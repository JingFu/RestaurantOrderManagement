<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
   version="2.0">
   <navigation-rule>
      <navigation-case>
         <from-outcome>order</from-outcome>
         <to-view-id>/secured/order.xhtml</to-view-id>
      </navigation-case>
   </navigation-rule>
   <navigation-rule>
      <navigation-case>
         <from-outcome>register_success</from-outcome>
         <to-view-id>/register_success.xhtml</to-view-id>
      </navigation-case>
   </navigation-rule>
    <navigation-rule>
      <navigation-case>
         <from-outcome>processing</from-outcome>
         <to-view-id>/secured/orderProcessing.xhtml</to-view-id>
      </navigation-case>
   </navigation-rule>
   <navigation-rule>
      <navigation-case>
         <from-outcome>delivery</from-outcome>
         <to-view-id>/secured/orderDelivery.xhtml</to-view-id>
      </navigation-case>
   </navigation-rule>
    <navigation-rule>
      <navigation-case>
         <from-outcome>search</from-outcome>
         <to-view-id>/secured/orderSearch.xhtml</to-view-id>
      </navigation-case>
   </navigation-rule>
    <navigation-rule>
      <navigation-case>
         <from-outcome>logout</from-outcome>
         <to-view-id>index.xhtml</to-view-id>
      </navigation-case>
   </navigation-rule>
   <application>
   	  <!--  
      <locale-config>
         <default-locale>en</default-locale>
         
         <supported-locale>en</supported-locale>
         <supported-locale>en_US</supported-locale>
         <supported-locale>zh_CN</supported-locale>
      </locale-config>
      -->
      <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
      <resource-bundle>
         <base-name>com.corejsf.messages</base-name>
         <var>msgs</var>
      </resource-bundle>
   </application>
</faces-config>